@model List<MyMedia.Models.Music.MusicListViewModel>
@using Microsoft.AspNetCore.Identity
@using MyMedia.Core.User;
@inject SignInManager<MediaUser> SignInManager
@inject UserManager<MediaUser> UserManager



<div class="container">
    <h2>Music <i class="fas fa-angle-double-right"></i>Index</h2>
    <hr /><p>
    </p>
    @if (Model.Any() && Model != null)
    {
        @foreach (var music in Model)
        {
            <partial name="_Summary" model="music" />
        }
    }
    else
    {
        <div class="alert alert-info" role="alert">
            Currently no Songs for display.
        </div>

    }

    <div class="row justify-content-end">
        <a asp-action="Create" class="btn btn-primary" asp-controller="Songs">Add Song</a>
    </div>

</div>
