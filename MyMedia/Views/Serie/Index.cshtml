@model List<MyMedia.Models.Series.SerieListViewModel>
@using Microsoft.AspNetCore.Identity
@using MyMedia.Core.User;
@inject SignInManager<Profiel> SignInManager
@inject UserManager<Profiel> UserManager
<div class="container">
    <h2>Serie <i class="fas fa-angle-double-right"></i>Index</h2>
    <hr />

    @if (Model != null && Model.Any())
    {
        foreach (var serie in Model)
        {
            <partial name="_Summary" model="serie" />
        }
    }
    else
    {
        <div class="alert alert-info" role="alert">
            Currently no Serie for display.
        </div>

    }
    <div class="row justify-content-end">
        <a asp-action="Create" class="btn btn-primary" asp-controller="Serie">Add Serie</a>
    </div>


</div>



